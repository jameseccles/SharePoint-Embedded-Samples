{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "postCreateCommand": "bash .devcontainer/spe-typescript-react-azurefunction/postCreate.sh",
  "postStartCommand": "bash .devcontainer/spe-typescript-react-azurefunction/postStart.sh",
  "forwardPorts": [7072, 8080],
  "portsAttributes": {
    "7072": {
      "label": "Function API"
    },
    "8080": {
      "label": "React Client"
    }
  }
}
